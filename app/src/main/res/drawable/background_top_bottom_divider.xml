        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/divider_width" android:color="@color/color_fafafa" />
            <solid android:color="@color/white" />

        </shape>
    </item>

    <item android:top="@dimen/divider_width" android:bottom="@dimen/divider_width">
