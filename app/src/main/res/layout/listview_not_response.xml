<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="45dp"
    android:background="@color/white">
    <ImageView
        android:id="@+id/not_respond_question_mark"
        android:layout_width="13dp"
        android:layout_height="10dp"
        android:layout_centerVertical="true"
        android:background="@drawable/red_question_mark"/>
    <TextView
        android:id="@+id/not_respond_listview_name"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:textColor="@color/text_enable"
        android:layout_toRightOf="@id/not_respond_question_mark"
        />

</RelativeLayout>